{
  "name": "zvelte",
  "displayName": "Zvelte Language Features",
  "publisher": "zvelte",
  "icon": "icons/logo.png",
  "description": "Language supports for Zvelte",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/PivotStick/zvelte-tools"
  },
  "contributes": {
    "languages": [{
      "id": "zvelte",
      "aliases": ["Zvelte", "zvelte"],
      "extensions": [".zvelte"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "zvelte",
      "scopeName": "source.zvelte",
      "path": "./syntaxes/zvelte.tmLanguage.json",
      "injectTo": ["./syntaxes/zvelte.tmLanguage.json"],
      "embeddedLanguages": {
        "source.css": "css",
        "source.css.scss": "scss",
        "source.js": "javascript",
        "source.ts": "typescript"
      }
    }]
  }
}
